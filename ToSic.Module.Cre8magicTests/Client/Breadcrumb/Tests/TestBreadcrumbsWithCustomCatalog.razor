@inject IMagicAct MagicAct
<h3>Tes tLanguages With Custom Spells Book</h3>
@code
{
    /// <inheritdoc cref="Oqtane.UI.PageState"/>
    [CascadingParameter] public required PageState PageState { get; set; }
}
@{
    var book = new MagicSpellsBook
    {
        Breadcrumbs = new()
        {
            {
                "myTest", new()
                {
                    WithHome = false,
                    WithActive = true
                }
            }
        }
    };

    var bcKit = MagicAct.BreadcrumbKit(((new MagicBreadcrumbSpell() with { PageState = PageState }).UseSpellsBook(book)));
}

